		</portlet-info>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>

	<portlet>
		<description>首页-能效分析</description>
		<portlet-name>efficiencyanalysis</portlet-name>
		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
